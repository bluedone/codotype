{
    "name": "@codotype/runtime",
    "version": "0.8.0-canary.10",
    "description": "Runtime environment that registers and runs Codotype Plugins",
    "scripts": {
        "test": "jest",
        "build": "npm run clean && npm run compile",
        "dev": "tsc -w -p tsconfig.build.json",
        "clean": "rm -rf ./dist",
        "compile": "tsc -p tsconfig.build.json",
        "prepublishOnly": "npm run build",
        "prettify": "prettier --config ../../../.prettierrc.js 'src/**/*.ts' --write"
    },
    "main": "dist/index",
    "types": "dist/index",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codotype/codotype.git"
    },
    "author": "Alexander Schwartzberg",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/codotype/codotype/issues"
    },
    "homepage": "https://github.com/codotype/codotype#readme",
    "dependencies": {
        "@codotype/core": "^0.8.0-canary.17",
        "ejs": "^2.6.1",
        "fs-extra": "^7.0.1",
        "chalk": "^2.4.1",
        "prettier": "^2.1.2"
    },
    "devDependencies": {
        "@types/ejs": "^2.6.3",
        "@types/fs-extra": "^9.0.2",
        "@types/node": "^12.12.12",
        "@types/prettier": "^2.0.2",
        "@types/jest": "^24.0.23",
        "jest": "^24.9.0",
        "ts-jest": "^24.2.0",
        "typescript": "~3.4.5"
    }
}
