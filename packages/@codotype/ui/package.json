{
    "name": "@codotype/ui",
    "version": "0.8.0-canary.60",
    "description": "Codotype React.js Components",
    "scripts": {
        "build": "npm run clean && npm run compile && npm run move-css",
        "dev": "tsc -w -p tsconfig.build.json",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "clean": "rm -rf ./dist",
        "compile": "tsc -p tsconfig.build.json",
        "move-css": "cp ./src/components/Story/tailwind.css ./ui.css",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codotype/codotype.git"
    },
    "keywords": [
        "react",
        "codotype",
        "typescript",
        "tailwindcss"
    ],
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "files": [
        "dist/src",
        "tailwind.config.js",
        "ui.css"
    ],
    "author": "Alexander Schwartzberg",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/codotype/codotype/issues"
    },
    "homepage": "https://github.com/codotype/codotype#readme",
